<div id="reg_form" class="bg_form">
  <h1>Relacion Cliente Usuario</h1>
  <p>
    <b>Usuario:</b>
    <%= @client_user.user_id %>
  </p>
  <p>
    <b>Cliente:</b>
    <%= @client_user.client_id %>
  </p>
  <p>
    <b>Fecha de vinculacion:</b>
    <%= @client_user.entailment_date %>
  </p>
  <p>
    <b>Fecha de salida:</b>
    <%= @client_user.exit_date %>
  </p>
  <p>
    <b>Estado:</b>
    <%= @client_user.user_state_id %>
  </p>
  <hr>
  <%= link_to 'Editar', edit_client_user_path(@client_user) %> |
  <%= link_to 'Regresar', client_users_path %>
</div>